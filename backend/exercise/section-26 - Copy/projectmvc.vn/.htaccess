RewriteEngine On



RewriteRule ^cart/([a-zA-Z0-9_-]+)-([0-9]+)$ index.php?mod=cart&controller=index&action=add&id=$2

RewriteRule ^category/([a-zA-Z0-9_-]+)-([0-9]+).html$ index.php?mod=product&controller=index&action=main&cat_id=$2

RewriteRule ^([a-zA-Z0-9_-]+)-([0-9]+).html$ index.php?mod=page&controller=index&action=detail&id=$2

RewriteRule ^product/([a-zA-Z0-9_-]+)-([0-9]+).html$ index.php?mod=product&controller=index&action=detail&id=$2




